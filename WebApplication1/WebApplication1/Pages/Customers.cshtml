@page
@model WebApplication1.Pages.CustomersModel
@{
}

<h1>Customers</h1>

<div>
    <table class="table table-striped">
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Age</th>
        </tr>
        @foreach (var item in Model.Customers)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Email</td>
                <td>@item.Age</td>
            </tr>
        }
    </table>
</div>